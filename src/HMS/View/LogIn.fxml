<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.GridPane?>
<GridPane stylesheets="@logIn.css" hgap="20" vgap="20" nodeOrientation="RIGHT_TO_LEFT" xmlns:fx="http://javafx.com/fxml/1"
          xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="HMS.Control.LogInController">
    <Label fx:id="titleLabel" text="لوحة تسجيل الدخول" GridPane.rowIndex="1" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.halignment="CENTER"/>
    <Label GridPane.rowIndex="2" GridPane.columnIndex="1" text="اسم المستخدم"/>
    <TextField fx:id="usernameField" GridPane.rowIndex="2" GridPane.columnIndex="2"/>
    <Label GridPane.rowIndex="3" GridPane.columnIndex="1" text="كلمة السر"/>
    <PasswordField fx:id="passwordField" GridPane.rowIndex="3" GridPane.columnIndex="2"/>
    <Button onAction="#logIn" fx:id="loginButton" GridPane.rowIndex="4" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.halignment="CENTER"
            text="تسجيل الدخول"/>

</GridPane>
